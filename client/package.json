{
  "name": "spotify-stats",
  "version": "0.1.0",
  "private": true,
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": "14.15.0"
  },
  "babelMacros": {
    "styledComponents": {
      "pure": true
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "homepage": "https://spotify-profile.herokuapp.com",
  "proxy": "http://localhost:8888",
  "lint-staged": {
    "*.{js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged"
    }
  },
  "dependencies": {
    "@babel/plugin-syntax-jsx": "^7.12.1",
    "@reach/router": "^1.2.1",
    "axios": "^0.21.0",
    "babel-plugin-macros": "^2.6.1",
    "chart.js": "^2.8.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "styled-components": "^5.2.1",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@upstatement/eslint-config": "^0.4.2",
    "babel-plugin-styled-components": "^1.10.6",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5"
  }
}
